<html>
  <head>
    <link rel="stylesheet" href="/path/to/tailwind.css" />
    <script src="/path/to/handlebars.js"></script>
  </head>
  <body>
    <div class="container">
      <h1 class="text-4xl font-bold text-center">{{largeNumber}}</h1>
      <form>
        <input type="text" placeholder="Item name" />
        <input type="text" placeholder="Cost" />
        <input type="text" placeholder="Merchant" />
        <input type="text" placeholder="Category" />
        <button type="submit">Submit</button>
      </form>
      <table>
        <tr>
          <th>Item</th>
          <th>Cost</th>
          <th>Merchant</th>
          <th>Category</th>
        </tr>
        {{#each entries}}
          <tr>
            <td>{{item}}</td>
            <td>{{cost}}</td>
            <td>{{merchant}}</td>
            <td>{{category}}</td>
          </tr>
        {{/each}}
      </table>
    </div>
  </body>
  <head>
    <script
      type="text/javascript"
      src="https://www.gstatic.com/charts/loader.js"
    ></script>
    <script type="text/javascript">
      google.charts.load("current", {packages:["corechart"]});
      google.charts.setOnLoadCallback(drawChart); function drawChart()
       { var data = google.visualization.arrayToDataTable([
        ['Spending', 'Amount per Category'],
        ['Work', 11], 
        ['Eat', 2], 
        ['Commute', 2], 
        ['Watch TV', 2],
        ['Sleep', 7] ]); var options = { title: 'My Monthly Spending', pieHole:
      0.4, }; var chart = new
      google.visualization.PieChart(document.getElementById('donutchart'));
      chart.draw(data, options); }
    </script>
  </head>
  <body>
    <div id="donutchart" style="width: 900px; height: 500px;"></div>
  </body>
</html>